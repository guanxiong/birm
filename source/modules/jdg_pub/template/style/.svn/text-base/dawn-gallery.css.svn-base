.tab-links-wrap{
    width: 100%;
    overflow-x: auto;
    background-color: rgba(255,255,255,0.3);
	-webkit-overflow-scrolling: touch;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
}

ul.tab-links{
	height: 44px;
	min-width: 100%;
	overflow: hidden;
}

ul.tab-links li{
    display: inline-block;
    float: left;
    font-size: 18px;
    line-height: 32px;
    height: 32px;
    padding: 6px 10px;
    padding-bottom: 4px;
    color: white;
}

ul.tab-links li a{
	color: rgba(255,255,255,0.6);
	display: block;
}

ul.tab-links li.active{
    border-bottom: 2px white solid;
}

ul.tab-links li.active a{
    color: white;
}

.dawn-gallery.dawn-gallery-theme-a{
    text-align: center;
}

.dawn-gallery.dawn-gallery-theme-a >.dawn-gallery-item{
    display: inline-block;
    width: 47%;
    margin-top: 4%;
    float: left;
    position: relative;
}

.dawn-gallery.dawn-gallery-theme-a >.dawn-gallery-item:nth-of-type(odd){
    margin-left: 2%;
    margin-right: 1%;
}

.dawn-gallery.dawn-gallery-theme-a >.dawn-gallery-item:nth-of-type(even){
    margin-left: 1%;
    margin-right: 2%;
}

.dawn-gallery.dawn-gallery-theme-a >.dawn-gallery-item img{
    width: 100%;
    height: auto;
    display: block;
}

.dawn-gallery.dawn-gallery-theme-a >.dawn-gallery-item p{
    position: absolute;
    bottom: 0;
    font-size: 12px;
    line-height: 250%;
    color: white;
    background-color: rgba(0,0,0,0.7);
    width: 100%;
}

.dawn-gallery.dawn-gallery-theme-a >.dawn-gallery-item p span.title{
    float: left;
    margin-left: 5px;
}

.dawn-gallery.dawn-gallery-theme-a >.dawn-gallery-item p span.price{
    float: right;
    margin-right: 5px;
}

.dawn-gallery-item-details .descripe >p:nth-of-type(1){
    font-size: 15px;
    line-height: 2;
}

.dawn-gallery-item-details .descripe >p:nth-of-type(1) >span.title {
    float: left;
}

.dawn-gallery-item-details .descripe >p:nth-of-type(1) >span.price{
    float: right;
}

.dawn-gallery-item-details .descripe >p:nth-of-type(2){
    font-size: 13px;
    line-height: 1.5;
}

#slider-view{
    background-color: black;
    position: fixed;
    width: 100%;
    height: 100%;
    overflow: hidden;
    top: 0;
    left: 0;
    z-index: 10;
    display: none;
}

.slider-view .swipe{
    position: relative;
    display: -webkit-box;
    display: box;
    width: 100%;
    height: 100%;
}

.slider-view .slider-item{
    text-align: center;
    position: relative; 
}

.slider-view .slider-item >img{
    max-width: 97%;
    max-height: 100%;
    vertical-align: middle;
}

.slider-view .slider-item >.descripe{
    position: absolute;
    bottom: 0;
    padding: 10px;
    color: white;
    background-color: rgba(0,0,0,0.6);
    text-align: left;
    width: 100%;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}

.swipe {
  overflow: hidden;
  visibility: hidden;
  position: relative;
}
.swipe-wrap {
  overflow: hidden;
  position: relative;
}
.swipe-wrap >div {
  float:left;
  width:100%;
  position: relative;
  overflow: hidden;
}

#page-ctrl-wrap{
    margin-top: 30px;
}

#page-ctrl-wrap .page-ctrl{
    margin: 10px;
}

.slider-view .slider-item >.descripe{
    position: absolute;
