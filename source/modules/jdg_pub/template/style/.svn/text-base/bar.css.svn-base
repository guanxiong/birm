html{
	min-height: 100%;
}

body{
	font-family: arial, helvetica, 'microsoft yahei', sans-serif;
	background-color: black;
	padding-bottom: 80px;
}

nav.app{
	position: fixed;
	bottom: 0;
	height: 50px;
	width: 100%;
	background-color: rgba(0,0,0,0.9);
	z-index: 999;
}

nav.app >span{
	display: block;
	width: 15%;
	height: 50px;
	background-repeat: no-repeat;
	background-position: 50% 50%;
	background-size: 25px 25px;
}

nav.app >span.nav-prev{
	float: left;
	background-image: url(icon-arrow-left.png);
}

nav.app >span.nav-next{
	float: right;
	background-image: url(icon-arrow-right.png);
}

nav.app .swipe{
  position: relative;
  float: left;
  width: 70%;
  height: 50px;
  overflow: hidden;
}

nav.app .swipe .swipe-wrap{
  	overflow: hidden;
  	position: relative;
}

nav.app ul{
	float: left;
	width: 100%;
	position: relative;
}

nav.app ul li{
	position: relative;
	display: block;
	float: left;
	width: 25%;
}

nav.app ul li a{
	display: block;
	float: left;
	width: 100%;
	height: 50px;
	position: relative;
	text-align: center;
	color: #ff3052;
}

nav.app ul li a span{
	height: 26px;
	width: 26px;
	display: inline-block;
	border-radius: 13px;
	margin-top: 4px;
	background-color: #ff3052;
}

nav.app ul li.actived a span{
	background-color: white;
}

nav.app ul li.actived a{
	color: white;
}

nav.app ul li a span img{
	width: 100%;
}

nav.app ul li a p{
	padding-top: 2px;
	font-size: 12px;
}

img.index-post{
	width: 100%;
}


.party-time-axis{
	display: -webkit-box;
	 -webkit-box-orient: horizontal;
	 margin-top: 20px;
	 margin-bottom: 30px;
}

.party-time-axis >div{
	width: 0;/*css3鐩掑瓙妯″瀷鐨刡ug淇*/
}

.party-time-axis >div >div {
	height: 230px;
	position: relative;
	margin-top: 20px;
}

/*浼樺寲涓夋槦Note3绛夊ぇ灞忕殑鏄剧ず*/
@media screen and (min-width: 480px){
	.party-time-axis >div >div {
	height: 350px;
	margin-top: 25px;
}
}

.party-time-axis .box-1{
	-webkit-box-flex: 1;
	border-right: 2px #ff3052 solid;
}

div.dot{
	width: 10px;
	height: 10px;
	border-radius: 5px;
	background-color: #ff3052;
	position: absolute;
	top: 0;
	left: 100%;
	margin-left: -4px;
}

.party-time-axis .box-2{
	-webkit-box-flex: 3;
}

div.party-brief p{
	font-size: 12px;
	width: 70px;
	margin: 0 auto;
	text-align: left;
}

.party-brief p.party-date{
	font-size: 12px;
	color: #ff3052;
	margin-bottom: 5px;
}

.party-brief p.party-subject{
	font-size: 12px;
	color: #c9c9c9;
	line-height: 135%;
}

div.party-post{
	overflow: hidden;
}

div.party-post img{
	width: 80%;
	display: block;
	margin: 0 auto;
}

header.activity a{
	position: absolute;
	display: block;
	width: 30px;
	height: 30px;
	background-color: #ff3052;
	top: 10px;
	left: 10px;
}

header.activity a img{
	width: 100%;
	border: 0;
}

header.activity p.subject{
	font-size: 16px;
	color: white;
	text-align: center;
	margin-top: 10px;
}

header.activity p.date{
	font-size: 12px;
	color: rgba(255,255,255,0.6);
	text-align: center;
	margin-top: 10px;
}

section.activity{
	padding: 10px;
	border-top: 1px #4d4d4d solid;
	margin-top: 15px;
}

section.activity >p{
	font-size: 14px;
	line-height: 135%;
	color: rgba(255,255,255,0.6);
	word-break: break-all;
	margin-bottom: 15px;
}

section.activity >p a{
	color: rgba(255,255,255,0.6);
}

#post-slider.swipe{
	overflow: hidden;
	visibility: hidden;
	position: relative;
	margin-top: 10px;
}

#post-slider.swipe .swipe-wrap{
	overflow: hidden;
	position: relative;
	max-height: 250px;
}

#post-slider.swipe-wrap >img{
	float: left;
	width: 100%;
	position: relative;
}

section#top button{
	width: 280px;
	height: 40px;
	display: block;
	margin: 15px auto;
	border-radius: 3px;
	border: 0;
	padding: 0;
	background-color: #cccccc;
}

section#top button span{
	vertical-align: top;
}

section#top button >span:nth-of-type(1){
	font-size: 20px;
	line-height: 40px;
	color: #ff3052;
	margin: 0 5px;
}

section#top button >span:nth-of-type(2){
	font-size: 14px;
	line-height: 40px;
	color: black;
	margin: 0 5px;
}


#headimg-slider.swipe{
	overflow: hidden;
	visibility: hidden;
	position: relative;
	margin-top: 10px;
}

#headimg-slider.swipe[data-item=three]{
	margin-left: 10%;
	margin-right: 10%;
}

#headimg-slider.swipe .swipe-wrap{
	overflow: hidden;
	position: relative;
}

#headimg-slider.swipe .swipe-wrap .swipe-item{
	position: relative;
	float: left;
	height: 230px;
	display: -webkit-box;
	-webkit-box-orient: vertical;
}

#headimg-slider.swipe .swipe-wrap .swipe-item ul{
	-webkit-box-flex: 1;
	display: -webkit-box;
	-webkit-box-orient: horizontal;
}

#headimg-slider.swipe .swipe-wrap 
.swipe-item ul li{
	-webkit-box-flex: 1;
	text-align: center;
	width: 0;
}

img.headimg{
	width: 64px;
	height: 64px;
	border-radius: 50%;
	display: inline-block;
	background-color: transparent;
}

#headimg-slider.swipe .swipe-wrap 
.swipe-item ul li >p.nickname{
	font-size: 12px;
	color: white;
	text-align: center;
	margin: 10px 0 20px;
}

section#comments article{
	display: -webkit-box;
	-webkit-box-orient: horizontal;
	margin-top: 15px;
}

section#comments article img.headimg{
	margin-right: 16px;
}

section#comments article .text{
	-webkit-box-flex: 1;
}

section#comments article .text p.caption{
	font-size: 14px;
	color: rgba(255,255,255,0.5);
}

section#comments article .text p.content{
	font-size: 14px;
	color: rgba(255,255,255,1);
	margin-top: 8px;
	line-height: 135%;
}

section#comments form{
	margin-top: 15px;
}

section#comments form textarea{
	background-color: white;
	width: 100%;
	border-radius: 3px;
	padding: 5px;
	border: 0;
	font-size: 14px;
	color: rgba(0,0,0,0.8);
	-webkit-appearance: none;
	resize: none;
	box-sizing: border-box;
}

section#comments form input[type=submit]{
	width: 85%;
	height: 40px;
	display: block;
	margin: 10px auto;
	background-color: #cccccc;
	color: #ff3052;
	font-size: 18px;
	line-height: 40px;
	text-align: center;
	border-radius: 3px;
	font-family: 'microsoft yahei';
	padding: 0;
	border: 0;
	-webkit-appearance: none;
}

label.error{
	position: fixed;
	width: 80%;
	padding: 10px 0;
	margin: 0 10%;
	top: 50%;
	-webkit-transform: translateY(-50%);
	transform: translateY(-50%);
	border-radius: 3px;
	font-size: 16px;
	color: red;
	text-align: center;
	z-index: 9999;
	background-color: rgba(255,255,255,0.9);
}

section#people-nearby{
	text-align: center;
}

