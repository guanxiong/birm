	word-break: normal;
}

article h3.time{
	line-height: 16px;
	vertical-align: middle;
	padding-top: 6px;
	color: #8c8c8c;
	font-size: 11px;
}

article p{
	font-size: 16px;
	line-height: 1.5;
	margin: 12px 0;
	color: #3e3e3e;
}

article img{
	max-width: 100%;
}

article ol,
article ul{
	font-size: 16px;
	line-height: 1.5;
	margin: 12px 0;
	color: #3e3e3e;
	padding-left: 22px;
}

